# Example .pypirc configuration for Nexus
# Copy this to ~/.pypirc and update with your credentials

[distutils]
index-servers =
    nexus
    pypi

[nexus]
repository = https://your-nexus-server.com/repository/pypi-private/
username = your-username
password = your-password

[pypi]
repository = https://upload.pypi.org/legacy/
username = __token__
password = pypi-token-here
